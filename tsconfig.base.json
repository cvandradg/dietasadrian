{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@classes/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/classes/*"
      ],
      "@decorators/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/decorators/*"
      ],
      "@dialogs/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/dialogs/*"
      ],
      "@dietas-adrian-nx-workspace/dietasadrian/frontend/ui/dietas": [
        "libs/dietasadrian/frontend/ui/dietas/src/index.ts"
      ],
      "@dietas-adrian-nx-workspace/dietasadrian/frontend/ui/landing-page": [
        "libs/dietasadrian/frontend/ui/landing-page/src/index.ts"
      ],
      "@dietasadrian/frontend/app": [
        "libs/dietasadrian/frontend/ui/app/src/index.ts"
      ],
      "@enviroments/*": ["apps/dietasadrian/src/environments/*"],
      "@guards/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/guards/*"
      ],
      "@helperFunctionsService": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/services/helperFunctions.service.ts"
      ],
      "@interfaces/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/interfaces/*"
      ],
      "@libs/dietas/*": [
        "libs/dietasadrian/frontend/ui/dietas/src/lib/*"
      ],
      "@libs/landing-page/*": [
        "libs/dietasadrian/frontend/ui/landing-page/src/lib/*"
      ],
      "@materialFunctions": [
        "libs/dietasadrian/frontend/shared/materialFunctions.ts"
      ],
      "@services/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/services/*"
      ],
      "@shared-modules": [
        "libs/dietasadrian/frontend/shared/shared-module/src/index.ts"
      ],
      "@shared-modules/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/*"
      ],
      "@shared-modules/store/*": [
        "libs/dietasadrian/frontend/shared/shared-module/src/lib/+state/*"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
