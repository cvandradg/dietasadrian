{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dietas-adrian-nx-workspace/dietasadrian/frontend/ui/dietas": [
        "libs/dietasadrian/frontend/ui/dietas/src/index.ts"
      ],
      "@dietas-adrian-nx-workspace/dietasadrian/frontend/ui/landing-page": [
        "libs/dietasadrian/frontend/ui/landing-page/src/index.ts"
      ],
      "@helperFunctions": [
        "libs/dietasadrian/frontend/shared/helperFunctions.ts"
      ],
      "@libs/dietas/dietas.module": [
        "libs/dietasadrian/frontend/ui/dietas/src/lib/dietas.module"
      ],
      "@libs/dietas/landing-page.module": [
        "libs/dietasadrian/frontend/ui/landing-page/src/lib/landing-page.module"
      ],
      "@shared-modules": [
        "libs/dietasadrian/frontend/shared/shared-module/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
